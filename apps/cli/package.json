{
  "name": "@catalyst/cli",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "catalyst": "./src/index.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "bun test",
    "test:unit": "bun test $(find . \\( -name '*.test.ts' -o -name '*.spec.ts' \\) ! -path '*/node_modules/*' | grep -vE 'integration|container')",
    "test:integration": "bun test $(find . \\( -name '*.test.ts' -o -name '*.spec.ts' \\) ! -path '*/node_modules/*' | grep -E 'integration|container')"
  },
  "dependencies": {
    "capnweb": "catalog:",
    "chalk": "catalog:",
    "commander": "catalog:",
    "@hono/node-server": "catalog:",
    "hono": "catalog:",
    "inquirer": "catalog:",
    "ws": "catalog:",
    "zod": "catalog:",
    "@catalyst/orchestrator-service": "catalog:",
    "@catalyst/routing": "catalog:",
    "@catalyst/authorization": "catalog:"
  },
  "devDependencies": {
    "@types/inquirer": "catalog:dev",
    "@types/node": "catalog:dev",
    "@types/ws": "catalog:dev",
    "@types/bun": "catalog:dev",
    "testcontainers": "catalog:testing",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
