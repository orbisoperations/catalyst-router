  "devDependencies": {
    "@types/node": "catalog:dev",
    "capnweb": "catalog:",
    "jose": "catalog:",
    "testcontainers": "catalog:testing",
    "typescript": "catalog:dev",
    "vitest": "catalog:testing"
  }
}
